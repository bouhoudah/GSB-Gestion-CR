@model IEnumerable<GSB_Gestion_CR.Models.Praticien>

@{
    ViewData["Title"] = "Index";
}

<h1>Praticiens</h1>

<p>
    <a asp-action="Create">Nouveaux</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.Label("Nom")
            </th>
            <th>
                @Html.Label("Prenom")
            </th>
            <th>
                @Html.Label("Addresse")
            </th>
            <th>
                @Html.Label("Code Postale")
            </th>
            <th>
                @Html.Label("Ville")
            </th>
            <th>
                @Html.Label("Coefficient")
            </th>
            <th>
              @*   @Html.DisplayNameFor(model => model.SsmaTimeStamp) *@
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TypCodeNavigation.TypLibelle)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PraNom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PraPrenom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PraAdresse)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PraCp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PraVille)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PraCoefnotoriete)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SsmaTimeStamp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TypCodeNavigation.TypCode)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PraNum">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PraNum">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PraNum">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
