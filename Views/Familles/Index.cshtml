@model IEnumerable<GSB_Gestion_CR.Models.Famille>

@{
    ViewData["Title"] = "Index";
}

<h1>Famille de Medicament</h1>

<p>
    <a asp-action="Create">Nouveaux</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FamLibelle)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FamLibelle)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FamCode">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FamCode">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FamCode">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
